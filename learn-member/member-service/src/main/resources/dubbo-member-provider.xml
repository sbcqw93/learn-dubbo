<?xml version="1.0" encoding="UTF-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:duboo="http://code.alibabatech.com/schema/dubbo"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://code.alibabatech.com/schema/dubbo
       http://code.alibabatech.com/schema/dubbo/dubbo.xsd">

    <duboo:application name="member-service-provider"/>

    <duboo:registry address="zookeeper://192.168.0.242:2181" timeout="3000" />

    <duboo:protocol accesslog="true" name="dubbo" port="9000"/>
    
    <duboo:service interface="com.ilearn.dubbo.member.service.ext.MemberExtService" ref="memberService"/>

</beans>